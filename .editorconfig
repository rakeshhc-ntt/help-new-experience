# EditorConfig is awesome: https://EditorConfig.org
# To use EditorConfig inside VS Code, please use the EditorConfig for VS Code plugin: https://marketplace.visualstudio.com/items?itemName=EditorConfig.EditorConfig

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2
